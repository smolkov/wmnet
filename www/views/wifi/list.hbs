{{#*inline "page"}}
    {{#if nets}}
        <div class="relative md:pt-32 pb-32 pt-12">
            <div class="px-2 md:px-10 mx-auto w-full">
                <div>
                    <div class="flex flex-wrap">
                        {{#each nets}}
                            <div class="w-full lg:w-6/12 xl:w-3/12 px-1 py-1">
                                <div
                                    class="relative flex flex-col min-w-0 break-words bg-white rounded mb-3 xl:mb-0 shadow-md">
                                    <a class="text-gray-800 hover:text-gray-600 text-xs uppercase font-bold block"
                                        href="/wifi/{{this.ssid}}">
                                        <div class="flex-auto p-2">
                                            <div class="flex flex-wrap">
                                                <div class="relative w-full pr-2 max-w-full flex-grow flex-1">
                                                    <p class="text-gray-500 normal-case">
                                                        {{this.quality}}
                                                    </p>
                                                    <p class="text-lg font-bold normal-case">
                                                        {{this.ssid}}
                                                    </p>
                                                </div>
                                            </div>
                                            <p class="text-sm text-gray-500 mt-4">
                                                <span class="text-green-500 mr-2">
                                                    <i class="fas fa-arrow-up"></i> {{this.channel}}
                                                </span>
                                                <span class="whitespace-no-wrap">
                                                    {{this.security}}
                                                </span>
                                                <span class="whitespace-no-wrap">
                                                    {{this.frequency}}
                                                </span>
                                            </p>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    {{/if}}
    {{!-- <a href="/channels/new">Create new city</a> --}}

{{/inline}}
{{! remove whitespaces with ~ }}
{{~> (parent)~}}